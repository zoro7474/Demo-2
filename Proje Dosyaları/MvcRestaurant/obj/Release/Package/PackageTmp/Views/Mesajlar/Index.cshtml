@using MvcRestaurant.Models.Entity
@model List<TBLMESAJLAR>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutPanelim.cshtml";
}

<h2>Mesajlar Sayfasi</h2>
<br />
<table class="table table-bordered">
    <tr>
        <th>Gönderen</th>
        <th>Konu</th>
        <th>Mesaj</th>
        <th>Tarih</th>
    </tr>
    @foreach(var x in Model)
    {
<tr>
    <td>@x.GONDEREN</td>
    <td>@x.KONU</td>
    <td>@x.İCERİK</td>
    <td>@x.TARİH</td>
</tr>
    }
</table>

