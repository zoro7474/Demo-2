@model MvcRestaurant.Models.Entity.TBLSEF

@{
    ViewBag.Title = "SefGetir";
    Layout = "~/Views/Shared/Layout.cshtml";
}

@using (Html.BeginForm("SefGüncelle", "Sefler", FormMethod.Post))
{
    <div style="margin-bottom:15px"></div>
    <div class="form-group">
        <div style="margin-bottom:15px">
            <label>Sef ID</label>
            @Html.TextBoxFor(y => y.ID, new { @class = "form-control" })
        </div>
        <div style="margin-bottom:15px">
            <label>Sef ADI</label>
            @Html.TextBoxFor(y => y.AD, new { @class = "form-control" })
        </div>

        <div style="margin-bottom:15px">
            <label>Sef ID</label>
            @Html.TextBoxFor(y => y.SOYAD, new { @class = "form-control" })
        </div>
        <div style="margin-bottom:15px">
            <label>Sef Detay</label>
            @Html.TextBoxFor(y => y.DETAY, new { @class = "form-control" })
        </div>
        <button class="btn btn-warning">Şef Bilgilerini Güncelle</button>
    </div>

}

